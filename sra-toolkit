#%Module######################################################################
source /opt/Modules/extensions/extensions.tcl

proc ModulesHelp { } {
        global version
        puts stderr "Provides NCBI's sra toolkit."
}

module-whatis   "sra-toolkit -- tools for extracting data from the SRA"

set PREFIX /g/data1/xe2/opt/apps/sra-toolkit

prepend-path PATH             $PREFIX/bin
prepend-path MANPATH          $PREFIX/share/man
