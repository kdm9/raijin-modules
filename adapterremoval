#%Module######################################################################
source /opt/Modules/extensions/extensions.tcl

proc ModulesHelp { } {
        global version
        puts stderr "Provides Adapter Removal."
}

module-whatis   "Adapter Removal -- Fast removal of adaptors"

set PREFIX /g/data1/xe2/opt/apps/adapterremoval

soft-prereq zlib/1.2.8
soft-prereq bzip2/1.0.6

prepend-path PATH             $PREFIX/bin
prepend-path MANPATH          $PREFIX/share/man
