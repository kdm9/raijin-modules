#%Module######################################################################
proc ModulesHelp { } {
        global version
        puts stderr "Provides BWA."
}

module-whatis   "bwa -- Short read aligner"

set XE2PATH /g/data1/xe2/opt/apps/bwa

prepend-path PATH             $XE2PATH/bin
prepend-path MANPATH          $XE2PATH/share/man
