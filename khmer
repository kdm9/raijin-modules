#%Module######################################################################
proc ModulesHelp { } {
        global version
        puts stderr "Provides khmer 2.0"
}

module-whatis   "khmer -- Probablistic k-mer counting and more"

set XE2PATH /g/data1/xe2/opt/apps/khmer

prepend-path PATH             $XE2PATH/installed-bin
prepend-path LD_LIBRARY_PATH  /apps/python3/3.5.2/lib
