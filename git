#%Module######################################################################
source /opt/Modules/extensions/extensions.tcl

proc ModulesHelp { } {
        global version
        puts stderr "Provides git."
}

module-whatis   "git and git-lfs"

set PREFIX /g/data1/xe2/opt/apps/git/

prepend-path PATH             $PREFIX/bin
prepend-path MANPATH          $PREFIX/share/man
